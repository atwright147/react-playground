    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react": "^3.1.0",
    "jsdom": "^22.1.0",
    "patch-package": "^6.5.1",
    "stubby": "^5.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  },
  "overrides": {
    "react-dnd-accessible-backend": {
      "react": "$react",
      "react-dnd": "$react-dnd"
