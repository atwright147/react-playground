    "classnames": "^2.3.2",
    "klona": "^2.0.6",
    "ky": "^0.33.3",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-accessible-backend": "^1.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^8.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.9.0",
    "react-use": "^17.4.0",
    "react-use-click-away": "^1.0.10",
    "reactflow": "^11.6.1",
    "sass": "^1.60.0",
    "uuid": "^9.0.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.6"
